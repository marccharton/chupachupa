<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ChupaChupa-Entities"
                   namespace="ChupaChupa.Entities">

  <class name="Cloud" table="CLOUD">
    <id name="IdEntity" column="IDCLOUD" unsaved-value="0">
      <generator class="native" />
    </id>

    <property name="Domain" column="DOMAIN"/>
    <property name="Port" column="PORT"/>
    <property name="Path" column="PATH"/>
    <property name="RegisterProcedure" column="REGISTERPROCEDURE"/>
    <property name="Protocol" column="PROTOCOL"/>
  </class>

</hibernate-mapping>


